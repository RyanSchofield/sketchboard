{
  "name": "sketchboard2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./src/server/server.ts",
  "scripts": {
    "start": "npm run dev-node",
    "dev-node": "npx concurrently -n server,client -c red,blue \"npm run dev-server-node\" \"npm run dev-client\"",
    "dev-server-node": "npx -T tsx watch ./src/server/server.node.ts",
    "dev-client": "vite --port $PORT --host",
    "test-ci": "echo 'No tests yet'",
    "test": "yarn run -T jest --passWithNoTests",
    "test-coverage": "lazy inherit",
    "lint": "yarn run -T tsx ../../scripts/lint.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@supabase/supabase-js": "^2.45.3",
    "@tldraw/sync": "^2.4.4",
    "@tldraw/sync-core": "^2.4.4",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "itty-router": "^5.0.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.1",
    "tldraw": "^2.4.4",
    "tsx": "^4.19.1",
    "unfurl.js": "^6.4.0",
    "vite": "^5.3.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "lazyrepo": "0.0.0-alpha.27",
    "typescript": "^5.3.3",
    "vite": "^5.3.4"
  }
}
